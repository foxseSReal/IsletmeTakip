<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colors -->
    <!--Renk Eklerken `SolidColoBrush` unutma-->
    <Color x:Key="Yellow">#E0BC00</Color>
    <Color x:Key="YellowTertiaty">#FED429</Color>
    <Color x:Key="Yellow100">#D9B423</Color>
    <Color x:Key="Yellow200">#BF9F1F</Color>
    <Color x:Key="Yellow300">#A58A1B</Color>
    <Color x:Key="Amber">#FFBF00</Color>
    <Color x:Key="Brass">#E1C16E</Color>
    <Color x:Key="Brass300">#D4A42B</Color>
    <Color x:Key="Grey200">#C4C4C4</Color>
    <Color x:Key="Grey300">#ADADAD</Color>
    <Color x:Key="Grey400">#969696</Color>
    <Color x:Key="Grey500">#808080</Color>
    <Color x:Key="Grey600">#696969</Color>
    <Color x:Key="Grey700">#525252</Color>
    <Color x:Key="Grey800">#3B3B3B</Color>
    <Color x:Key="Grey900">#242424</Color>
    <Color x:Key="Charcoal">#36454F</Color>
    <Color x:Key="Brick">#903909</Color>
    <!-- Brushes -->
    <SolidColorBrush x:Key="Dark100YellowBrush" Color="{StaticResource Yellow}"/>
    <SolidColorBrush x:Key="YellowTertiatyBrush" Color="{StaticResource YellowTertiaty}"/>
    <SolidColorBrush x:Key="Yellow100Brush" Color="{StaticResource Yellow100}"/>
    <SolidColorBrush x:Key="Yellow200Brush" Color="{StaticResource Yellow200}"/>
    <SolidColorBrush x:Key="Yellow300Brush" Color="{StaticResource Yellow300}"/>
    <SolidColorBrush x:Key="AmberBrush" Color="{StaticResource Amber}"/>
    <SolidColorBrush x:Key="BrassBrush" Color="{StaticResource Brass}"/>
    <SolidColorBrush x:Key="Brass300Brush" Color="{StaticResource Brass300}"/>
    <SolidColorBrush x:Key="Grey200Brush" Color="{StaticResource Grey200}"/>
    <SolidColorBrush x:Key="Grey300Brush" Color="{StaticResource Grey300}"/>
    <SolidColorBrush x:Key="Grey400Brush" Color="{StaticResource Grey400}"/>
    <SolidColorBrush x:Key="Grey500Brush" Color="{StaticResource Grey500}"/>
    <SolidColorBrush x:Key="Grey600Brush" Color="{StaticResource Grey600}"/>
    <SolidColorBrush x:Key="Grey700Brush" Color="{StaticResource Grey700}"/>
    <SolidColorBrush x:Key="Grey800Brush" Color="{StaticResource Grey800}"/>
    <SolidColorBrush x:Key="Grey900Brush" Color="{StaticResource Grey900}"/>
    <SolidColorBrush x:Key="CharcoalBrush" Color="{StaticResource Charcoal}"/>
    <SolidColorBrush x:Key="BrickBrush" Color="{StaticResource Brick}"/>
</ResourceDictionary>